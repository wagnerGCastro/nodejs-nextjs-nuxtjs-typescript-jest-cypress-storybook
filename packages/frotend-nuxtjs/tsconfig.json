{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./src/*"
      ],
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      // "node",
      "@nuxt/types",
      // "@types/node",
      // "@nuxtjs/axios",
      // "cypress",
      "jest",
      // "vuetify",
      // "@types/jest",
    ]
  },

  //"include": ["**/*.ts", "**/*.js", "**/*.vue"],
  // "include": ["src/**/*", "__tests__/**/*"],
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist",
    "cypress",
  ]
}
